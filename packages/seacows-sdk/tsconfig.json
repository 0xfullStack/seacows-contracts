{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "dist",
    "allowJs": true,
    "checkJs": false,
    "target": "es5",
    "allowUnreachableCode": false,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "useUnknownInCatchVariables": false,
    "declaration": true,
    "strictNullChecks": true,
    "esModuleInterop": true
  },
  "include": [
      "./**/*"
  ]
}