{
  "name": "@yolominds/seacows-sdk",
  "version": "1.4.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "copy:amm": "copyfiles -u 3 ../seacows-amm/types/**/*.d.ts ../seacows-amm/types/**/*.ts ./types/amm && copyfiles -u 3 ../seacows-amm/abis/**/* ./abis/amm",
    "copy:periphery": "copyfiles -u 3 ../seacows-periphery/types/**/*.d.ts ../seacows-periphery/types/**/*.ts ./types/periphery && copyfiles -u 3 ../seacows-periphery/abis/**/* ./abis/periphery",
    "release": "npx standard-version --message 'chore(release): %s\n\n[skip ci]' -t @yolominds/seacows-sdk\\@ --no-verify && npm publish"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "ts-node": "^10.7.0",
    "typescript": "^5.0.4"
  },
  "files": [
    "dist/**/*",
    "abis/**/*"
  ]
}
